<?xml version="1.0" encoding="utf-8"?>
<mx:Button xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="15" cornerRadius="0" alpha=".6" fontSize="8" textAlign="left" icon="@Embed(source='../graph_small.jpg')" color="#A6A6A6">
	<mx:Script>
		<![CDATA[
			import mx.charts.series.LineSeries;
			import mx.managers.PopUpManager;
			import mx.containers.Canvas;
			import mx.collections.ArrayCollection;
			public var graph:ArrayCollection;
			public var canvas_Main:Canvas;
			public var graphName:String;
			
			public function drawThisGraph(e:MouseEvent):void
			{
				var graphPopUp:GraphViewPop=GraphViewPop(PopUpManager.createPopUp(canvas_Main, GraphViewPop , true));
	        	graphPopUp.dataCollection=graph;
	        	PopUpManager.centerPopUp(graphPopUp);
	        	var mySeries:Array=new Array();
		        var series:LineSeries = new LineSeries();
		        series.yField=graphName;
		        series.displayName=graphName;
		        mySeries.push(series);
				graphPopUp.graphLineChart.series= mySeries;
			}
		]]>
	</mx:Script>
</mx:Button>
