<?xml version="1.0" encoding="utf-8"?>
<mx:TitleWindow xmlns:mx="http://www.adobe.com/2006/mxml" layout="absolute" width="800" height="410" title="Graph Viewer" showCloseButton="true" close="closePop(event);" modalTransparency=".5" modalTransparencyBlur="4" modalTransparencyColor="#E8E8E8" modalTransparencyDuration="200">
	<mx:Script>
		<![CDATA[
			import mx.managers.PopUpManager;
			import mx.collections.ArrayCollection;
			[Bindable]
			public var dataCollection:ArrayCollection;
			private function closePop(e:Event):void
			{
				PopUpManager.removePopUp(this);
			}
		]]>
	</mx:Script>

	<mx:LineChart width="100%" height="100%" x="90" y="0" id="graphLineChart" dataProvider="{dataCollection}" >

		<mx:horizontalAxis>
            <mx:CategoryAxis title="DATA SIZE" categoryField="month" />
        </mx:horizontalAxis>
        <mx:verticalAxis>
            <mx:LinearAxis title="TIME"/>
        </mx:verticalAxis>
	</mx:LineChart>
	<mx:Legend dataProvider="{graphLineChart}"/>
	
</mx:TitleWindow>
